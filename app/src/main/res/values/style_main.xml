<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ContentGeneralMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/background_temporal</item>
    </style>

    <style name="FrameContentMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_above">@+id/nav_view_custom</item>
    </style>

    <style name="ChipContentMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:elevation">16dp</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@color/white</item>
    </style>



</resources>